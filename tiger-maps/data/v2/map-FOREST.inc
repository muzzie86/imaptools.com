  # map-FOREST.inc
  LAYER
    NAME "Forest"
    GROUP "cb"
    STATUS DEFAULT
    TYPE POLYGON
    TILEINDEX "not-nt2/tidx-ForestBoundary"
    PROJECTION "init=epsg:4326" END
    MAXSCALEDENOM 200000000
    LABELITEM "NAME"
    CLASS
      STYLE
        COLOR "#C9DFAF"
      END
      LABEL
        MAXSCALEDENOM 10000000
        ENCODING "UTF8"
        TYPE TRUETYPE FONT "arial" SIZE 8 COLOR "#24481C"
        PARTIALS FALSE OUTLINECOLOR "#F4F3F0" OUTLINEWIDTH 1
        BUFFER 4 POSITION AUTO PRIORITY 3 # MINDISTANCE 250
        MINFEATURESIZE 30 WRAP ' ' ALIGN CENTER
      END
    END
  END
